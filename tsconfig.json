{
	"compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": "./",
        "paths":{
            "stores": ["src/stores/index.ts"],
            "utils": ["src/utils/index.ts"]
        },
		"sourceMap": true,
		"noImplicitAny": true,
        "module": "commonjs",
		"target": "es5",
        "jsx": "react",
        "lib": ["es6", "dom"],
        "experimentalDecorators": true
	},
	"include": [
		"./src/**/*"
	]
}
